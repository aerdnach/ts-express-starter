{
  "name": "ts-starter",
  "version": "1.0.0",
  "description": "A starter for TypeScript.",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon",
    "build": "rimraf dist && tsc -p .",
    "start": "npm run build && node -r ts-node/register/transpile-only -r tsconfig-paths/register src/index.ts",
    "test": "jest",
    "eslint": "eslint 'src/**/*.ts'",
    "eslint:fix": "eslint --fix 'src/**/*.ts'",
    "prettier": "prettier 'src/**/*.ts'",
    "prettier:fix": "prettier --write 'src/**/*.ts'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@ef-carbon/tspm": "^2.2.5",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.4.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.3.0"
  },
  "dependencies": {
    "express": "^4.17.1",
    "rimraf": "^3.0.2"
  }
}
